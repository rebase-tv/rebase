---
import Layout from "@layouts/main.astro"
import Container from "@components/container.astro"
import Logo from "@components/logo.astro"
import Message from "@components/message.astro"
import { Auth } from "sst/node/auth"
import { Image } from "@astrojs/image/components"
import frame from "@images/phone-frame.svg"

const messages = [
  { avatar: "/images/dax.png", handle: "thdxr", message: "too easy" },
  { avatar: "/images/trash.png", handle: "trashh_dev", message: "help" },
  {
    avatar: "/images/bash.jpg",
    handle: "sudobunni",
    message: "eek js question",
  },
  { avatar: "/images/theo.png", handle: "t3dotgg", message: "starts with a p" },
  {
    avatar: "/images/prime.png",
    handle: "ThePrimeagen",
    message: "yea it does",
  },
  {
    avatar: "/images/ken.png",
    handle: "ken_wheeler",
    message: "lol",
  },
  { avatar: "/images/bash.jpg", handle: "sudobunni", message: "PRIME" },
]

const auth =
  Auth.auth.url +
  "/authorize?" +
  new URLSearchParams({
    client_id: "local",
    redirect_uri: Astro.url.origin + "/auth/callback",
    response_type: "code",
  }).toString()
